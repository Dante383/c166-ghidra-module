# ADDB Rbn, Rbm
:addb rbn, rbm is op=0x01 & rbn & rbm & ExtCountDec {
	Add(rbn, rbm);
}

# ADDB Rbn, [Rwi]
# ADDB Rbn, [Rwi+]
# ADDB Rwn, #data3
:addb rbn, IndexImmB is op=0x09 & rbn & IndexImmB & ExtCountDec {
	Add(rbn, IndexImmB);
}

# ADDB reg, #data8
:addb ShortMemAddrB, DataImmB is op=0x07 & ShortMemAddrB; DataImmB & ExtCountDec {
	Add(ShortMemAddrB, DataImmB);
}

# ADDB reg, mem
:addb ShortMemAddrB, LongMemAddrB is op=0x03 & ShortMemAddrB; LongMemAddrB & ExtCountDec {
	Add(ShortMemAddrB, LongMemAddrB);
}

# ADDB mem, reg
:addb LongMemAddrB, ShortMemAddrB is op=0x05 & ShortMemAddrB; LongMemAddrB & ExtCountDec {
	Add(LongMemAddrB, ShortMemAddrB);
}

