# JB bitaddrQ.q, rel
:jb BitoffAddr^"."bit_q, rel1623 is op=0x8A & BitoffAddr; bit_z = 0 & bit_q & rel1623 & ExtCountDec {
	local bit:2 = BitoffAddr & (1 << bit_q);
	if (!(bit == (1 << bit_q))) goto inst_next;
	goto rel1623;
}

# ------

# JNB bitaddrQ.q, rel
:jnb BitoffAddr^"."bit_q, rel1623 is op=0x9A & BitoffAddr; bit_z = 0 & bit_q & rel1623 & ExtCountDec {
