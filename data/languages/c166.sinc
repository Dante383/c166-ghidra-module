define endian=$(ENDIAN);

define alignment=2;

define space ram type=ram_space size=3 default;
define space register type=register_space size=2;

define pcodeop segment;
define pcodeop GetPagedOffset;
define pcodeop GetPagedOffsetInd;
define pcodeop SetExtpIndCallOther;
define pcodeop SetExtsIndCallOther;

define register offset=0 size=2
	[ r0 r1 r2 r3 r4 r5 r6 r7 r8 r9 r10 r11 r12 r13 r14 r15 IP ];
define register offset=0 size=1
	[ RL0 RH0 RL1 RH1 RL2 RH2 RL3 RH3 RL4 RH4 RL5 RH5 RL6 RH6 RL7 RH7 ];

define ram offset=0xFE00 size=2
	[ DPP0 DPP1 DPP2 DPP3 ];

define register offset=0xFE08 size=2
	[ CSP _ MDH MDL CP SP STKOV STKUN ONES ZEROS ];

define register offset=0xFF10 size=2 
	[ MDC PSW SYSCON ];
	
define register offset=0x50 size=4 contextreg;
define context contextreg
	Counter = (0,2)
	ExtrEn  = (3,3)
	ExtpEn  = (4,4)
	ExtsEn  = (5,5)
	Extp    = (6,21)
	Exts    = (22, 29)
;

@define PSW_N "PSW[0,1]"
@define PSW_C "PSW[1,1]"
@define PSW_V "PSW[2,1]"
@define PSW_Z "PSW[3,1]"
@define PSW_E "PSW[4,1]"

macro setPreFlags(a, b) {
	$(PSW_C) = ((a + b) < a) | ((a + b) < b);
	$(PSW_V) = ((a s< 0) == (b s< 0)) & (((a + b) s< 0) != (a s< 0));
	$(PSW_E) = (b == 0x8000);
}

macro setPostFlags(result) {
	$(PSW_N) = (result s< 0);
	$(PSW_Z) = (result == 0x0);
}

macro setPreFlagsCarry(a, b) {
	local lastCarry = $(PSW_C);
	$(PSW_C) = ((a + b + zext(lastCarry)) < a) | ((a + b + zext(lastCarry)) < b) | (zext(lastCarry) & ((a + b) == 0xFFFF));
	$(PSW_V) = ((a s< 0) == (b s< 0)) & (((a + b + zext(lastCarry)) s< 0) != (a s< 0));
	$(PSW_E) = (b == 0x8000);
}

macro setPostFlagsCarry(result) {
	local lastZero = $(PSW_Z);
	$(PSW_N) = (result s< 0);
	$(PSW_Z) = (result == 0x0) && (lastZero);
}

macro setPreFlagsAnd(b) {
	$(PSW_C) = 0;
	$(PSW_V) = 0;
	$(PSW_E) = (b == 0x8000);
}

macro setPreFlagsSub(a, b) {
	$(PSW_C) = a < b;
	$(PSW_V) = ((a s< b) != ((a s< 0) != (a s< 0)));
	$(PSW_E) = (b == 0x8000);
}

macro setPreFlagsSubCarry(a, b) {
	local lastCarry = $(PSW_C);
	$(PSW_C) = ((a - zext(lastCarry)) < b) | (a < zext(lastCarry));
	$(PSW_V) = ((a s< b) != ((a s< 0) != (b s< 0))) | (a < zext(lastCarry));
	$(PSW_E) = (b == 0x8000);
}

macro setPreFlagsAshr(a, b) {
	$(PSW_C) = (b != 0) && ((a & (1 << (b-1))) > 1);
	$(PSW_V) = (b != 0) && ((a s>> b) << b) != a;
	$(PSW_E) = 0;
}

macro setPreFlagsShr(a, b) {
	$(PSW_C) = (b != 0) && ((a & (1 << (b-1))) > 1);
	$(PSW_V) = (b != 0) && ((a >> b) << b) != a;
	$(PSW_E) = 0;
}

macro setBitwiseFlags(a, b) {
	local tmpa:1 = a:1;
	local tmpb:1 = b:1;
	$(PSW_N) = tmpa ^ tmpb;
	$(PSW_C) = tmpa & tmpb;
	$(PSW_V) = tmpa | tmpb;
	$(PSW_Z) = !$(PSW_V);
	$(PSW_E) = 0;
}

macro setBitClearFlags(a) {
	$(PSW_N) = a == 1;
	$(PSW_C) = 0;
	$(PSW_V) = 0;
	$(PSW_Z) = !$(PSW_N);
	$(PSW_E) = 0;
}

macro setDivFlags(a) {
	$(PSW_C) = 0;
	$(PSW_V) = !(a == 0x0);
	$(PSW_E) = 0;
}

macro Push(a) {
	SP = SP - 2;
	local stack:3 = segment(0:2, SP);
	*stack = a;
}

macro Pop(a) {
	local stack:3 = segment(0:2, SP);
	a = *stack;
	SP = SP + 2;
}

macro ReturnIntra() {
	Pop(IP);
	local currAddr:3 = inst_start;
	local jmpAddr:3 = (currAddr & 0xFF0000) | zext(IP);
	return [jmpAddr];
}


macro ReturnInter() {
	Pop(IP);
	Pop(CSP);
	local jmpAddr:3 = (zext(CSP) << 16) | zext(IP);
	return [jmpAddr];
}


define token instr_16(16)
	op        = (0, 7)
	op0003    = (0,3)
	cond0407  =(4,7)
	op_noop   = (0,15)
	bit_q_0407  = (4,7)
	rwn          = (12,15)
	rbn          = (12,15)
	cond            = (12,15)
	rwm          = ( 8,11)
	rbm          = ( 8,11)
	idx_imm 	 = (11,11)
	idx_mode	 = (10,10)
	data3		 = ( 8,10)
	data4        = (12,15)
	rwi			 = (8,9)
	irang2       = (12,13)
	irang_mode   = (14,15)
	rel       = (8, 15)  signed
	urel      = (8, 15)
	trap7     = (9, 15)  dec
	reg       = (8, 15)
	bitoff    = (8,14)
	bitoff_high_bit  =(15,15)
	reg_high = (12,15)
	reg_low = (8,11)
	reg_low_b = (8,11)
;

define token instr_16_2(16)
	caddr = (0,15)
	page  = (0,15)
	seg   = (0,15)
	data16 = (0, 15)
	mem = (0,15)
	z      = (4, 7)
	z_high_bit = (7,7)
	z_2    = (0, 3)
	bitaddr_z = (0,6)
	bit_z  = (8,11)	    dec
	bit_q  = (12,15)	dec
	mask8 = (8,15)
	rel_32 = (0,7)	signed
;

define token instr_32(32)
	op_32 = (0,31)
;

attach variables [ rwn rwm reg_low z z_2 ] [ r0 r1 r2 r3 r4 r5 r6 r7 r8 r9 r10 r11 r12 r13 r14 r15 ];
attach variables [ rbn rbm reg_low_b ] [ RL0 RH0 RL1 RH1 RL2 RH2 RL3 RH3 RL4 RH4 RL5 RH5 RL6 RH6 RL7 RH7 ];
attach variables [ rwi ] [ r0 r1 r2 r3 ];

DataImmW: "#"^data16  is data16 { local tmp:2 = data16; export tmp; }
DataImmB: "#"^data16  is data16 { local tmp:1 = data16:1; export tmp; }

IndexImmW: [rwi] is idx_imm=1 & idx_mode = 0 & rwi { local tmp:3 = GetPagedOffsetInd(rwi); export *:2tmp; }
IndexImmW: [rwi^"+"] is idx_imm=1 & idx_mode = 1 & rwi { local tmp:3 = GetPagedOffsetInd(rwi); rwi = rwi + 2; export *:2tmp; }
IndexImmW: "#"^data3 is idx_imm=0 & data3 { local tmp:2 = zext(data3:1); export tmp; }
IndexImmB: [rwi] is idx_imm=1 & idx_mode = 0 & rwi { local tmp:3 = GetPagedOffsetInd(rwi); export *:1tmp; }
IndexImmB: [rwi^"+"] is idx_imm=1 & idx_mode = 1 & rwi { local tmp:3 = GetPagedOffsetInd(rwi); rwi = rwi + 1; export *:1tmp; }
IndexImmB: "#"^data3 is idx_imm=0 & data3 { local tmp:1 = data3:1; export tmp; }


ShortMemAddrW: regoff is ExtrEn=0 & reg_high < 0xf & reg [ regoff = 0xFE00 + (2 * reg); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
ShortMemAddrW: regoff is ExtrEn=1 & reg_high < 0xf & reg [ regoff = 0xF000 + (2 * reg); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
ShortMemAddrW: reg_low is reg_high=0xf & reg_low { export reg_low; }
ShortMemAddrB: regoff is ExtrEn=0 & reg_high < 0xf & reg [ regoff = 0xFE00 + (2 * reg); ] { local tmp:3 = zext(regoff:2); export *:1tmp; }
ShortMemAddrB: regoff is ExtrEn=1 & reg_high < 0xf & reg [ regoff = 0xF000 + (2 * reg); ] { local tmp:3 = zext(regoff:2); export *:1tmp; }
ShortMemAddrB: reg_low_b is reg_high=0xf & reg_low_b { export reg_low_b; }

BitoffAddr: regoff is ExtrEn=0 & bitoff_high_bit=0 & bitoff [ regoff = 0xFD00 + (2 * bitoff); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddr: regoff is ExtrEn=0 & bitoff_high_bit=1 & bitoff [ regoff = 0xFF00 + (2 * bitoff); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddr: regoff is ExtrEn=1 & bitoff_high_bit=1 & bitoff [ regoff = 0xF100 + (2 * bitoff); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddr: reg_low is reg_high=0xf & reg_low { export reg_low; }

BitoffAddrZ: regoff is ExtrEn=0 & z_high_bit=0 & bitaddr_z [ regoff = 0xFD00 + (2 * bitaddr_z); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddrZ: regoff is ExtrEn=0 & z_high_bit=1 & bitaddr_z [ regoff = 0xFF00 + (2 * bitaddr_z); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddrZ: regoff is ExtrEn=1 & z_high_bit=1 & bitaddr_z [ regoff = 0xF100 + (2 * bitaddr_z); ] { local tmp:3 = zext(regoff:2); export *:2tmp; }
BitoffAddrZ: z_2 is z=0xf & z_2 { export z_2; }


SetAtomic: "#"^range is irang2 [ range = irang2 + 1; Counter = range; globalset(inst_next, Counter); ] { local tmp:1 = range:1; export tmp; }
SetExtr: SetAtomic is SetAtomic [ ExtrEn = 1; globalset(inst_next, ExtrEn); ] {}
SetExtpInd: rwm, SetAtomic is rwm & SetAtomic [ ExtpEn = 1; globalset(inst_next, ExtpEn); ] { SetExtpIndCallOther(rwm); }
SetExtp: page, SetAtomic is rwm=0 & SetAtomic; page [ ExtpEn = 1; globalset(inst_next, ExtpEn); Extp=page; globalset(inst_next, Extp); ] {}
SetExtsInd: rwm, SetAtomic is rwm & SetAtomic [ ExtsEn = 1; globalset(inst_next, ExtsEn); ] { SetExtsIndCallOther(rwm); }
SetExts: seg, SetAtomic is rwm = 0 & SetAtomic; seg [ ExtsEn = 1; globalset(inst_next, ExtsEn); Exts=seg; globalset(inst_next, Exts); ] {}
ExtCountDec: is Counter=0 [ ExtrEn=0; globalset(inst_next, ExtrEn); ExtpEn=0; globalset(inst_next, ExtpEn); ExtsEn=0; globalset(inst_next, ExtsEn); ] {}
ExtCountDec: is Counter!=0 [ Counter = Counter - 1; globalset(inst_next, Counter);] {}

LongMemAddrW: mem is ExtsEn=0 & ExtpEn=0 & mem { local tmp:3 = GetPagedOffset(mem:2); export *:2tmp; }
LongMemAddrW: memoff is ExtpEn=1 & mem & Extp [ memoff = (Extp<<14) | (mem & 0x3FFF); ] { local tmp:3 = memoff:3; export *:2tmp; }
LongMemAddrW: memoff is ExtsEn=1 & mem & Exts [ memoff = (Exts<<16) | (mem & 0x3FFF); ] { local tmp:3 = memoff:3; export *:2tmp; }

LongMemAddrB: mem is ExtsEn=0 & ExtpEn=0 & mem { local tmp:3 = GetPagedOffset(mem:2); export *:1tmp; }
LongMemAddrB: memoff is ExtpEn=1 & mem & Extp [ memoff = (Extp<<14) | (mem & 0x3FFF); ] { local tmp:3 = memoff:3; export *:1tmp; }
LongMemAddrB: memoff is ExtsEn=1 & mem & Exts [ memoff = (Exts<<16) | (mem & 0x3FFF); ] { local tmp:3 = memoff:3; export *:1tmp; }

RwnIndW: [rwn] is rwn { local tmp:3 = GetPagedOffsetInd(rwn); export *:2tmp; }
RwnIndWP: [rwn+] is rwn { local tmp:3 = GetPagedOffsetInd(rwn); rwn=rwn+2; export *:2tmp; }
RwmIndW: [rwm] is rwm { local tmp:3 = GetPagedOffsetInd(rwm); export *:2tmp; }
RwmIndWP: [rwm+] is rwm { local tmp:3 = GetPagedOffsetInd(rwm); rwm=rwm+2; export *:2tmp; }
RwmIndWM: [-rwm] is rwm { rwm=rwm-2; local tmp:3 = GetPagedOffsetInd(rwm); export *:2tmp; }
RwmIndWOff: [rwm+DataImmW] is rwm; DataImmW { local tmp:3 = zext(rwm) + zext(DataImmW); export *[ram]:2 tmp; }

RwnIndB: [rwn] is rwn { local tmp:3 = GetPagedOffsetInd(rwn); export *:1tmp; }
RwnIndBP: [rwn+] is rwn { local tmp:3 = GetPagedOffsetInd(rwn); rwn=rwn+1; export *:1tmp; }
RwmIndB: [rwm] is rwm { local tmp:3 = GetPagedOffsetInd(rwm); export *:1tmp; }
RwmIndBP: [rwm+] is rwm { local tmp:3 = GetPagedOffsetInd(rwm); rwm=rwm+1; export *:1tmp; }
RwmIndBM: [-rwm] is rwm { rwm=rwm-1; local tmp:3 = GetPagedOffsetInd(rwm); export *:1tmp; }
RwmIndBOff: [rwm+DataImmW] is rwm; DataImmW { local tmp:3 = zext(rwm) + zext(DataImmW); export *[ram]:1 tmp; }

RWnRWmEqual: is rwn=rwm {}

rel0815: addr is rel [ addr = inst_next + (2 * rel); ] { IP = rel; export *[ram]:2 addr;  }
rel1623: addr is rel_32 [ addr = inst_next + (2 * rel_32); ] { IP = rel_32; export *[ram]:2 addr;  }
relSeg: addr is urel; caddr [ addr = (urel << 16) | caddr; ] { IP=caddr:2; CSP=urel:2; export *[ram]:2 addr; }
absCaddr: addr is caddr [ addr = (inst_start & 0xFF0000) | caddr; ] { IP=caddr:2; export *[ram]:2 addr; }
trapAddr: "#"^addr is trap7 [ addr = trap7 << 2; ] { CSP=0; IP=addr:2; export *[ram]:2 addr;}

cc0407:"cc_UC"	is cond0407=0x0 { }
cc0407:"cc_NET"	is cond0407=0x1 { if   ($(PSW_Z) | $(PSW_E)) goto inst_next; }
cc0407:"cc_EQ"	is cond0407=0x2 { if   !$(PSW_Z) goto inst_next; }
cc0407:"cc_NE"	is cond0407=0x3 { if    $(PSW_Z) goto inst_next; }
cc0407:"cc_V"	is cond0407=0x4 { if   !$(PSW_V) goto inst_next; }
cc0407:"cc_NV"	is cond0407=0x5 { if    $(PSW_V) goto inst_next; }
cc0407:"cc_N"	is cond0407=0x6 { if   !$(PSW_N) goto inst_next; }
cc0407:"cc_NN"	is cond0407=0x7 { if    $(PSW_N) goto inst_next; }
cc0407:"cc_C"	is cond0407=0x8 { if   !$(PSW_C) goto inst_next; }
cc0407:"cc_NC"	is cond0407=0x9 { if    $(PSW_C) goto inst_next; }
cc0407:"cc_SGT"	is cond0407=0xA { if   ($(PSW_Z) | ($(PSW_N) ^ $(PSW_V))) goto inst_next; }
cc0407:"cc_SLE"	is cond0407=0xB { if  !($(PSW_Z) | ($(PSW_N) ^ $(PSW_V))) goto inst_next; }
cc0407:"cc_SLT"	is cond0407=0xC { if  !($(PSW_N) ^ $(PSW_V)) goto inst_next; }
cc0407:"cc_SGE"	is cond0407=0xD { if   ($(PSW_N) ^ $(PSW_V)) goto inst_next; }
cc0407:"cc_UGT"	is cond0407=0xE { if   ($(PSW_Z) | $(PSW_C)) goto inst_next; }
cc0407:"cc_ULE"	is cond0407=0xF { if  !($(PSW_Z) | $(PSW_C)) goto inst_next; }


cc1215:"cc_UC"	is cond=0x0 { }
cc1215:"cc_NET"	is cond=0x1 { if   ($(PSW_Z) | $(PSW_E)) goto inst_next; }
cc1215:"cc_EQ"	is cond=0x2 { if   !$(PSW_Z) goto inst_next; }
cc1215:"cc_NE"	is cond=0x3 { if    $(PSW_Z) goto inst_next; }
cc1215:"cc_V"	is cond=0x4 { if   !$(PSW_V) goto inst_next; }
cc1215:"cc_NV"	is cond=0x5 { if    $(PSW_V) goto inst_next; }
cc1215:"cc_N"	is cond=0x6 { if   !$(PSW_N) goto inst_next; }
cc1215:"cc_NN"	is cond=0x7 { if    $(PSW_N) goto inst_next; }
cc1215:"cc_C"	is cond=0x8 { if   !$(PSW_C) goto inst_next; }
cc1215:"cc_NC"	is cond=0x9 { if    $(PSW_C) goto inst_next; }
cc1215:"cc_SGT"	is cond=0xA { if   ($(PSW_Z) | ($(PSW_N) ^ $(PSW_V))) goto inst_next; }
cc1215:"cc_SLE"	is cond=0xB { if  !($(PSW_Z) | ($(PSW_N) ^ $(PSW_V))) goto inst_next; }
cc1215:"cc_SLT"	is cond=0xC { if  !($(PSW_N) ^ $(PSW_V)) goto inst_next; }
cc1215:"cc_SGE"	is cond=0xD { if   ($(PSW_N) ^ $(PSW_V)) goto inst_next; }
cc1215:"cc_UGT"	is cond=0xE { if   ($(PSW_Z) | $(PSW_C)) goto inst_next; }
cc1215:"cc_ULE"	is cond=0xF { if  !($(PSW_Z) | $(PSW_C)) goto inst_next; }


macro Add(a, b) {
	local tmpA = a;
	local tmpB = b;
	setPreFlags(tmpA, tmpB);
	local result = tmpA + tmpB;
	a = result;
	setPostFlags(result);
}

# ADD Rwn, Rwm
:add rwn, rwm is op=0x00 & rwn & rwm & ExtCountDec {
	Add(rwn, rwm);
}

# ADD Rwn, [Rwi]
# ADD Rwn, [Rwi+]
# ADD Rwn, #data3
:add rwn, IndexImmW is op=0x08 & rwn & IndexImmW & ExtCountDec {
	Add(rwn, IndexImmW);
}

# ADD reg, #data16
:add ShortMemAddrW, DataImmW is op=0x06 & ShortMemAddrW; DataImmW & ExtCountDec {
	Add(ShortMemAddrW, DataImmW);
}

# ADD reg, mem
:add ShortMemAddrW, LongMemAddrW is op=0x02 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
	Add(ShortMemAddrW, LongMemAddrW);
}

# ADD mem, reg
:add LongMemAddrW, ShortMemAddrW is op=0x04 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
	Add(LongMemAddrW, ShortMemAddrW);
}

