macro AddCarry(a, b) {
	local carr = $(PSW_C);
	local tmpA = a;
	local tmpB = b;
	setPreFlagsCarry(tmpA, tmpB);
	local result = tmpA + tmpB + zext(carr);
	a = result;
	setPostFlagsCarry(result);
}


# ADDC Rwn, Rwm
:addc rwn, rwm is op=0x10 & rwn & rwm & ExtCountDec {
	AddCarry(rwn, rwm);
}

# ADDC Rwn, [Rwi]
# ADDC Rwn, [Rwi+]
# ADDC Rwn, #data3
:addc rwn, IndexImmW is op=0x18 & rwn & IndexImmW & ExtCountDec {
	AddCarry(rwn, IndexImmW);
}

# ADDC reg, #data16
:addc ShortMemAddrW, DataImmW is op=0x16 & ShortMemAddrW; DataImmW & ExtCountDec {
	AddCarry(ShortMemAddrW, DataImmW);
}

# ADDC reg, mem
:addc ShortMemAddrW, LongMemAddrW is op=0x12 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
	AddCarry(ShortMemAddrW, LongMemAddrW);
}

# ADDC mem, reg
:addc LongMemAddrW, ShortMemAddrW is op=0x14 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
	AddCarry(LongMemAddrW, ShortMemAddrW);
}

