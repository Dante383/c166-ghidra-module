	And(ShortMemAddrW, DataImmW);
}

# AND reg, mem
:and [ShortMemAddrW], LongMemAddrW is op=0x62 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
	And(ShortMemAddrW, LongMemAddrW);
}

# AND mem, reg
:and LongMemAddrW, ShortMemAddrW is op=0x64 & ShortMemAddrW; LongMemAddrW & ExtCountDec {
