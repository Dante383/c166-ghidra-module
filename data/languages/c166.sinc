macro And(a, b) {
	local tmpA = a;
	local tmpB = b;
	setPreFlagsAnd(tmpB);
	local result = tmpA & tmpB;
	a = result;
	setPostFlags(result);
}

# AND Rwn, Rwm
:and rwn, rwm is op=0x60 & rwn & rwm & ExtCountDec {
	And(rwn, rwm);
}

# AND Rwn, [Rwi]
