# BFLDH bitoffQ, #mask8, #data8
:bfldh BitoffAddr, "#"^mask8, DataImmB is op=0x1A & BitoffAddr; DataImmB & mask8 & ExtCountDec {
	$(PSW_E) = 0;
	$(PSW_V) = 0;
	$(PSW_C) = 0;
	local hbyte:2 = BitoffAddr >> 8;
	hbyte = ((hbyte & (~zext(mask8:1))) | zext(DataImmB));
	BitoffAddr = (BitoffAddr & 0x00FF) | (hbyte << 8);
	setPostFlags(BitoffAddr);
} 

# ------

# BFLDL bitoffQ, #mask8, #data8
:bfldl BitoffAddr, "#"^mask8, DataImmB is op=0x0A & BitoffAddr; DataImmB & mask8 & ExtCountDec {
	$(PSW_E) = 0;
	$(PSW_V) = 0;
	$(PSW_C) = 0;
	local lbyte:2 = BitoffAddr & 0xFF00;
	lbyte = ((lbyte & (~zext(mask8:1))) | zext(DataImmB));
	BitoffAddr = (BitoffAddr & 0xFF00) | lbyte;
	setPostFlags(BitoffAddr);
} 

